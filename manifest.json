{
    "manifest_version": 3,
    "name": "CVNV Extension",
    "version": "1.0",
    "action": {
        "default_title": "Click to copy text"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "scripting",
        "storage",
        "activeTab"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}
